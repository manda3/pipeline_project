pipeline {
      agent any
         stages {
            stage('build') {
                   steps {
                         echo 'build the job'
                         }
                         }
            stage('compile') {
                    steps {
                          echo 'compile the job'
                          }
                          }
            stage('package'){
                           steps {
                                   echo "package the job"
                                 }
                          } 
         }
}
